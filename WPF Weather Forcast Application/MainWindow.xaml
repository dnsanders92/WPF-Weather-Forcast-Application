<Window x:Class="WPF_Weather_Forcast_Application.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WPF_Weather_Forcast_Application"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="700"
        Background="LightBlue">
    <Grid>
        <Label x:Name="LBL_City" Content="City" HorizontalAlignment="Left" Margin="113,217,0,0" VerticalAlignment="Top" FontSize="24" Width="196"/>
        <Label x:Name="LBL_CurrentTemp" Content="Temp" HorizontalAlignment="Left" VerticalAlignment="Top" Height="45" Width="168" Margin="113,259,0,0" FontSize="24"/>
        <Label x:Name="LBL_CurrentHumidity" Content="Humidity" HorizontalAlignment="Left" Margin="113,304,0,0" VerticalAlignment="Top"/>
        <Label x:Name="LBL_CurrentPressure" Content="Air Pressure" HorizontalAlignment="Left" Margin="113,335,0,0" VerticalAlignment="Top"/>
        <Label x:Name="LBL_WeatherConditions" Content="Conditions" HorizontalAlignment="Left" Margin="113,41,0,0" VerticalAlignment="Top"/>
        <Image x:Name="IMG_CurrentConditions" HorizontalAlignment="Left" Height="160" Margin="86,67,0,0" VerticalAlignment="Top" Width="165" Source="/placeholder.png"/>
        <Label x:Name="LBL_LowTemp1" Content="Low Temp" HorizontalAlignment="Left" Margin="523,84,0,0" VerticalAlignment="Top"/>
        <Label x:Name="LBL_HighTemp1" Content="High Temp" HorizontalAlignment="Left" Margin="427,84,0,0" VerticalAlignment="Top"/>
        <Label x:Name="LBL_HighTemp2" Content="High Temp" HorizontalAlignment="Left" Margin="427,134,0,0" VerticalAlignment="Top"/>
        <Label x:Name="LBL_HighTemp3" Content="High Temp" HorizontalAlignment="Left" Margin="427,180,0,0" VerticalAlignment="Top"/>
        <Label x:Name="LBL_HighTemp4" Content="High Temp" HorizontalAlignment="Left" Margin="427,225,0,0" VerticalAlignment="Top"/>
        <Label x:Name="LBL_HighTemp5" Content="High Temp" HorizontalAlignment="Left" Margin="427,268,0,0" VerticalAlignment="Top"/>
        <Label x:Name="LBL_LowTemp2" Content="Low Temp" HorizontalAlignment="Left" Margin="523,134,0,0" VerticalAlignment="Top"/>
        <Label x:Name="LBL_LowTemp3" Content="Low Temp" HorizontalAlignment="Left" Margin="523,180,0,0" VerticalAlignment="Top" RenderTransformOrigin="0.511,0.017"/>
        <Label x:Name="LBL_LowTemp4" Content="Low Temp" HorizontalAlignment="Left" Margin="523,225,0,0" VerticalAlignment="Top"/>
        <Label x:Name="LBL_LowTemp5" Content="Low Temp" HorizontalAlignment="Left" Margin="523,268,0,0" VerticalAlignment="Top"/>

    </Grid>
</Window>
